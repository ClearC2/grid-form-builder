{
  "name": "grid-form-builder",
  "version": "2.11.5",
  "description": "Form Builder built on top of React Grid Layout",
  "repository": {
    "type": "git",
    "url": "https://github.com/ClearC2/grid-form-builder"
  },
  "yarnVersion": "1.9.2",
  "main": "lib/index.js",
  "scripts": {
    "start": "concurrently -k -p \"[{name}]\" -n \"api,ui\" \"npm run api\" \"npm run ui\"",
    "ui": "cd example && cross-env webpack-dev-server --host 0.0.0.0 --inline --content-base dist --history-api-fallback --port 8777",
    "api": "cd example && cross-env NODE_ENV=server nodemon server/index.js --watch server",
    "build": "babel -d lib/ src/ --ignore '**/*.test.js'",
    "build:watch": "babel -d lib/ src/ --watch",
    "prepublish": "npm run build",
    "install:yarn": "./install-yarn.sh",
    "cm": "git-cz",
    "semantic-release": "semantic-release",
    "test": "jest",
    "lint": "eslint",
    "lint-fix": "eslint src/ --fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "branch": "master",
    "extends": "c2-semantic-release"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": "eslint",
    "server/**/*.js": "eslint",
    "test/**/*.js": "eslint"
  },
  "author": "Jacob Allen",
  "license": "ISC",
  "standard": {
    "parser": "babel-eslint",
    "env": [
      "jest"
    ],
    "globals": [
      "$",
      "global",
      "process"
    ],
    "ignore": [
      "/dist/",
      "/theme/",
      "/.yarn/",
      "/lib/"
    ]
  },
  "devDependencies": {
    "@clearc2/eslint-config-c2-react": "^2.0.0",
    "axios": "^0.18.0",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.6.0",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.16.0",
    "body-parser": "^1.18.2",
    "c2-dialog": "ClearC2/c2-dialog#^2.0.0",
    "c2-semantic-release": "ClearC2/c2-semantic-release#^2.0.0",
    "concurrently": "^3.5.1",
    "cors": "^2.8.4",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.1",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^1.1.6",
    "identity-obj-proxy": "^3.0.0",
    "immutable": "^3.8.2",
    "jest": "^23.0.0",
    "jest-dom": "^3.1.3",
    "lodash.delay": "^4.1.1",
    "nodemon": "^1.17.2",
    "react": "^16.12.0",
    "react-dnd-test-backend": "^9.4.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-testing-library": "^6.1.2",
    "redux": "^3.7.2",
    "redux-immutable": "^4.0.0",
    "resolve-url-loader": "^2.2.1",
    "sass-loader": "^6.0.6",
    "snazzy": "^7.1.1",
    "standard": "10.0.0",
    "style-loader": "^0.17.0",
    "url-loader": "^0.6.2",
    "webpack": "^2.5.0",
    "webpack-dev-server": "^2.4.5"
  },
  "peerDependencies": {
    "c2-dialog": "ClearC2/c2-dialog#^2.0.0",
    "immutable": "^3.0.0",
    "jquery": "^3.2.1 || ^3.3.1",
    "moment": "^2.20.1",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0",
    "react-redux": "^4.0.0 || ^5.0.0"
  },
  "dependencies": {
    "@hot-loader/react-dom": "^16.8.6",
    "cleave.js": "^1.5.3",
    "daterangepicker": "^3.0.5",
    "hoist-non-react-statics": "^3.3.0",
    "moment": "^2.20.1",
    "prop-types": "^15.0.0",
    "react-color": "^2.17.3",
    "react-dnd": "^9.4.0",
    "react-dnd-html5-backend": "^9.4.0",
    "react-grid-layout": "^0.17.1",
    "react-html-id": "^0.1.5",
    "react-icons": "^2.2.7",
    "react-portal": "^4.1.2",
    "react-quill": "^1.3.3",
    "react-select": "^3.0.8",
    "react-sizeme": "^2.6.10",
    "react-toggle-button": "^2.2.0",
    "yarn": "1.9.2"
  }
}

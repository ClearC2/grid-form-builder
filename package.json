{
  "name": "grid-form-builder",
  "version": "1.0.0",
  "description": "Form Builder built on top of React Grid Layout",
  "repository": {
    "type": "git",
    "url": "https://github.com/ClearC2/grid-form-builder"
  },
  "yarnVersion": "1.3.2",
  "main": "lib/index.js",
  "scripts": {
    "start": "concurrently -k -p \"[{name}]\" -n \"api,ui\" \"npm run api\" \"npm run ui\"",
    "ui": "cd example && cross-env webpack-dev-server --host 0.0.0.0 --inline --content-base dist --history-api-fallback --port 8777",
    "api": "cd example && cross-env NODE_ENV=server nodemon server/index.js --watch server",
    "compile": "babel -d lib/ src/",
    "prepublish": "npm run compile",
    "lint": "standard \"src/**/*.js\"",
    "install:yarn": "./install-yarn.sh"
  },
  "author": "Jacob Allen",
  "license": "ISC",
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "$",
      "global",
      "process"
    ],
    "ignore": [
      "/dist/",
      "/theme/",
      "/.yarn/",
      "/lib/"
    ]
  },
  "devDependencies": {
    "axios": "^0.18.0",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "7.0.0",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.18.2",
    "concurrently": "^3.5.1",
    "cors": "^2.8.4",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.1",
    "eslint": "3.19.0",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^1.1.6",
    "immutable": "^3.8.2",
    "lodash.delay": "^4.1.1",
    "nodemon": "^1.17.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-immutable": "^4.0.0",
    "resolve-url-loader": "^2.2.1",
    "query-builder": "git@github.com:ClearC2/query-builder.git",
    "sass-loader": "^6.0.6",
    "snazzy": "^7.1.1",
    "standard": "10.0.0",
    "style-loader": "^0.17.0",
    "url-loader": "^0.6.2",
    "webpack": "^2.5.0",
    "webpack-dev-server": "^2.4.5"
  },
  "peerDependencies": {
    "immutable": "^3.0.0",
    "moment": "^2.20.1",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0",
    "react-redux": "^4.0.0 || ^5.0.0"
  },
  "dependencies": {
    "c2-dialog": "git+ssh://git@github.com:ClearC2/c2-dialog.git",
    "cleave.js": "^1.1.1",
    "moment": "^2.20.1",
    "node-sass": "^4.7.2",
    "prop-types": "^15.0.0",
    "react-datetime": "^2.11.1",
    "react-dnd": "^2.5.4",
    "react-dnd-html5-backend": "^2.5.4",
    "react-grid-layout": "^0.15.2",
    "react-icons": "^2.2.7",
    "react-portal": "^4.1.2",
    "react-select": "^1.2.1",
    "react-sizeme": "2.4.1"
  }
}
